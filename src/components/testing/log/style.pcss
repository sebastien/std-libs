@import controls

.Log-
	.-title:
		margin-bottom: 10px
	.-items-
		border: 1px solid var(--border-color__lt)
		.-item__string
			padding-top: 5px
			padding-bottom: 5px
			padding-left: 15px
			border-top: 1px solid var(--border-color__lt)
			font-size: 80%
			&:first-child:
				border-top: 0px solid transparent
		.-item__object
			padding: 15px
			background-color: var(--page-color__alt)

